/*회사 안내 및 조직도*/

#Section-Company-Info{width:100%; height:627px; background:url(../images/back_info2.jpg) center no-repeat; }
#Section-Company-Info .Section-Company-Info-Contents{width:1200px; height:627px; margin:0 auto;}
 
.Section-Company-Info-Contents .Company-Info{width:600px; height:627px; float:left; position:relative;}
.Section-Company-Info-Contents .Organization-Chart{width:600px; height:627px; float:left;}



	

/*회사안내*/

.Section-Company-Info-Contents .Company-Info{width:590px; height:600px; float:left;   position:relative;}
.Section-Company-Info-Contents .Company-Info table{width:520px; height:auto; margin:30px; margin-top:30px; font-size:14px; font-weight:bold; color:#fff;}
.Section-Company-Info-Contents .Company-Info table th{background:url(../images/bullet_red.gif) no-repeat 0 25px ;   text-align:left; width:120px; text-indent:20px; padding:22px 0;}

.Section-Company-Info-Contents .Company-Info table td{text-align:left; }
.Section-Company-Info-Contents .Company-Info table td em{display:block; padding-bottom:5px;}

.Section-Company-Info-Contents .Company-Info p{position:absolute; right:15px; bottom:10px; width:213px; height:150px;}



/*조직도*/

.Section-Company-Info-Contents .Organization-Chart p img{display:block; width:528px; height:421px; margin:0 auto; margin-top:40px; }








/*사업소개*/



#Section-Business-Info{overflow:hidden; width:100%; height:630px; margin:0 auto; /* background:url(../images/back_business.jpg) center no-repeat;*/ }
#Section-Business-Info h3{ color:#000000;}
#Section-Business-Info .Section-Business-Info-Con{width:1030px; height:580px; margin:0 auto;  }

#Section-Business-Info .Section-Business-Info-Con .Info-Bnr{width:292px; height:352px; background:#fff; float:left;  margin:0 25px; margin-top:50px; position:relative; box-shadow: 1px 1px 50px gray;}
	
			


#Section-Business-Info .Section-Business-Info-Con .Info-Bnr dl dd.Title{text-align:center; height:30px; font-weight:bold; color:green; margin-top:15px; font-size:14px;}
#Section-Business-Info .Section-Business-Info-Con .Info-Bnr dl dd.Business-Contents {width:95%; margin-top:3px; margin:0 auto; text-align:center; padding-top:15px; font-size:14px;}

#Section-Business-Info .Section-Business-Info-Con .Info-Bnr dl dd span{display:block; text-align:center; height:25px;}


/*사업소개 롤오버 아웃처리 */


#Section-Business-Info .Section-Business-Info-Con .Info-Bnr .Mask{width:292px; height:352px; position:absolute; top:0; left:0; background:url(../images/mask.png) center no-repeat; 
  -ms-filter: "progid: DXImageTransform.Microsoft.Alpha(Opacity=0)"; filter: alpha(opacity=0); opacity: 0; background-color: rgba(219,127,8, 0.7); -webkit-transition: all 0.4s ease-in-out; -moz-transition: all 0.4s ease-in-out; -o-transition: all 0.4s ease-in-out;
  -ms-transition: all 0.4s ease-in-out; transition: all 0.4s ease-in-out;
}

#Section-Business-Info .Section-Business-Info-Con .Info-Bnr .Mask p{width:250px; height:250px; margin:0 auto; text-align:center;  margin-top:75px; color:#fff; font-size:14px;}
#Section-Business-Info .Section-Business-Info-Con .Info-Bnr .Mask p span{display:block; margin-top:5px;}


/*사업소개 롤오버 처리*/

#Section-Business-Info .Section-Business-Info-Con .Info-Bnr .Mask:hover{-ms-filter: "progid: DXImageTransform.Microsoft.Alpha(Opacity=100)"; filter: alpha(opacity=100);opacity: 1;}





