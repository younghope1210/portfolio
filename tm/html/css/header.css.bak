
#header{width:100%; height:115px; background:#fff; position:fixed; z-index:1000; border-bottom:1px solid #3f32a2; }

#header .header-container{width:1200px; height:107px; margin:0 auto; }
#header .header-container .header-con{width:1200px; height:107px;  position:relative;}
#header #Util{width:100%; margin:0 auto; border-bottom:1px solid #262632; background:#262632; }
#header ul#utile-menu{width:1200px; height:30px;  margin:0 auto;}
#header ul#utile-menu li{width:72px; height:24px; float:right; text-align:right; font-family:dotum; margin-left:15px; }
#header ul#utile-menu li a{display:block; width:72px; height:24px; color:#fff; line-height:28px;}
#header .header-container .header-con h1{width:231px; height:75px; float:left; margin-top:15px; }
#header .header-container .header-con #gnb{width:880px; height:60px; float:right;  }
#header .header-container .header-con #gnb ul{width:880px; height:60px; margin-top:15px }
#header .header-container .header-con #gnb ul li{float:left; text-align:right;}
#header .header-container .header-con #gnb ul li a{display:block; color:#000; font-size:18px; width:176px; line-height:60px;}
#header .header-container .header-con #gnb ul li a:hover{color:#829913;}


