
/* 상단 유틸 메뉴 시작 */
#Util-Menu{width:100%; height:37px; background:#ff6e0d; margin:0 auto; font-family:dotum;  }
#Util-Menu .Util-Menu-Con{width:1200px; height:37px; margin:0 auto; background:url(../images/header/back_util_menu.gif) no-repeat; }
#Util-Menu .Util-Menu-Con ul.Util-Join{float:left;}
#Util-Menu .Util-Menu-Con ul.Util-Join img{width:20px; height:20px; margin-right:10px; vertical-align:middle; }
#Util-Menu .Util-Menu-Con ul li a{display:block; color:#fff; font-size:14px; font-weight:bold; line-height:37px;}

#Util-Menu .Util-Menu-Con ul.Util-Join li{float:left; width:105px; height:auto;}
#Util-Menu .Util-Menu-Con ul.Util-Info{float:right;}
#Util-Menu .Util-Menu-Con ul.Util-Info li{float:left; width:105px;}



/*로고및 GNB 시작*/

#gnb{width:100%; height:80px; background:#fff;}

#gnb .gnb-Con{width:1200px; height:224px; margin:0 auto;}
#gnb .gnb-Con h1{width:158px; height:69px; float:left; margin-top:7px;}
#gnb .gnb-Con ul{float:left; margin-left:50px; margin-top:23px;}  
#gnb .gnb-Con ul li{float:left; width:122px; height:38px; margin-left:18px; font-size:18px; }
#gnb .gnb-Con ul li a{width:122px; height:38px; line-height:38px; display:block; text-align:center; color:#000000;  }
#gnb .gnb-Con ul li a:hover{ background:url(../images/header/gnb_over_back.png) no-repeat; color:#fff;}





/*로고및 GNB 반응형 시작 */
#gnb2{width:100%; height:80px; background:#fff;  display:none;}

#gnb2 .gnb-Con{width:1200px; height:224px; margin:0 auto; position:relative;}
#gnb2 .gnb-Con h1{width:158px; height:69px; float:left; margin-top:7px;}
#gnb2 .gnb-Con .Btn-Gnb{float:right; width:120px; height:80px; line-height:80px; font-size:18px; background:#31a180; text-align:center; color:#fff;}


#gnb2 .gnb-Con ul{width:100%; height:auto; position:absolute; top:80px; left:0; background:#f85915;}  

#gnb2 .gnb-Con ul li{height:50px; text-align:center; font-size:14px; font-weight:bold; color:#fff; font-family:dotum;  border-bottom:1px solid #d14b12;}
#gnb2 .gnb-Con ul li a{display:block; height:50px; line-height:50px; color:#fff; }








