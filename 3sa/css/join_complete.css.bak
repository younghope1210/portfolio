

.Visual{width:100%; height:200px; margin:0 auto; overflow:hidden; background:url(../images/join/skin.jpg) no-repeat center top;}
.Visual p{width:1200px; margin:0 auto; font-size:40px; font-weight:bold; color:#fff; text-align:center; padding-top:70px; font-family: 'Noto Sans KR', sans-serif;}

/*공통*/
h3{width:100%; height:100px; border-bottom:1px solid #ccc; text-align:center; font-size:36px; font-weight:bold; color:#000;}
h3 img{width:60px; height:3px; margin:0 auto; border:0; vertical-align:bottom; margin-bottom:8px;}
h3 span.H3-Text{display:block; height:40px; text-align:center;}



/*로그인 페이지 시작*/

#Join-Complete {width:1200px; height:auto; margin:0 auto; }
#Join-Complete .Join-Complete-Con{width:1120px; height:auto; margin:0 auto; }


#Join-Complete .Join-Complete-Con .Join-Greeting{width:100%; height:210px; border-bottom:2px solid #f85915; margin-top:20px;}
#Join-Complete .Join-Complete-Con .Join-Greeting p.Skin-graphe{width:446px; height:203px; float:left;}


/*상단 인삿말 시작*/

#Join-Complete .Join-Complete-Con .Join-Greeting p.Greeting{width:660px;  height:132px; line-height:50px; float:right; font-size:40px; text-align:right; margin-top:50px; color:#666666; font-weight:bold;} 
#Join-Complete .Join-Complete-Con .Join-Greeting p.Greeting span{display:inline-block;  }
#Join-Complete .Join-Complete-Con .Join-Greeting p.Greeting span em{color:#ff6600;}


/*회원가입 완료 */


/*아이디 패스워드 찾기 시작*/


.Join-Complete-Contents{width:100%; height:500px; margin:0 auto;}
.Join-Complete-Contents p.Left-SKin{width:298px; height:385px; float:left; margin-top:50px; margin-left:60px; }

.Join-Complete-Contents .Complete-Greetion{width:670px; height:500px; float:right;  margin-top:60px;}
.Join-Complete-Contents .Complete-Greetion h4{font-size:50px; color:#000; margin-top:10px;}
.Join-Complete-Contents .Complete-Greetion h4 em.line-Block{display:block;  margin-bottom:10px;}
.Join-Complete-Contents .Complete-Greetion h4 em.Col-0c705a{color:#0c705a;}
.Join-Complete-Contents .Complete-Greetion h4 span{display:block; font-size:24px; margin:20px 0;}
.Join-Complete-Contents .Complete-Greetion ul{padding-top:20px;}
.Join-Complete-Contents .Complete-Greetion ul li{ background:url(../images/join/bullet_circle-orange.gif) 0 6px no-repeat; text-indent:15px; color:#000; font-size:18px; height:30px;}
.Join-Complete-Contents .Complete-Greetion ul li a{color:#f41224; text-decoration:underline; }
.Join-Complete-Contents .Complete-Greetion p.Btn-Login{width:294px; height:51px; margin-top:30px;}
