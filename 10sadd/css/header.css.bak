#header{width:100%; height:122px;  background:url(../images/header/back-line.gif) repeat-x; /*position:fixed; z-index:1000; opacity:0.9*/}

#header .header-container{width:1200px; height:107px; margin:0 auto; }
#header .header-container .header-con{width:1200px; height:107px;  position:relative;}
#header .header-container .header-con ul#utile-menu{width:400px; height:40px; float:right; }
#header .header-container .header-con ul#utile-menu li{width:72px; height:25px; float:left; text-align:center; font-family:dotum; margin-left:15px; }
#header .header-container .header-con ul#utile-menu li a{display:block; width:72px; height:24px; color:#fff; line-height:25px;}

#header .header-container .header-con h1{width:175px; height:69px; float:left; margin-top:50px;}

#header .header-container .header-con #gnb{width:880px; height:60px; float:right; margin-top:20px; }
#header .header-container .header-con #gnb ul{width:880px; height:60px;}
#header .header-container .header-con #gnb ul li{float:left; text-align:center;}
#header .header-container .header-con #gnb ul li a{display:block; color:#00; font-size:18px; width:140px; line-height:60px;}
#header .header-container .header-con #gnb ul li a:hover{color:#3fa789;}





