@charset "utf-8";







#container{ width:100%; height:auto; margin:0 auto; overflow:hidden; position:relative;}

#container #visual{width:100%; height:502px; background:#cbd1ce; margin:0 auto; overflow:hidden; position:relative;}
#container #visual h4{width:451px; height:249px; position:absolute; left:25%; top:150px; transform: translateX(-25%); z-index:100;}
#container #visual p{width:1920px; height:502px; margin:0 auto;  animation: leftMove 5s; transition: transform .15s; transform: scale(1.0);}


@keyframes leftMove {
	
	50% {
		transform: scale(1.2);
	}
	
	100% {
		transform: scale(1.0);
	}
}




