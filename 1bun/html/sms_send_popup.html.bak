.Brand-Check ul{margin-top:15px; margin-left:10px;}
.Brand-Check ul li{margin-bottom:5px;}
.Brand-Check ul li input{width:13px; height:13px; vertical-align:middle; margin-right:5px; border:1px solid }

/* 회원 정보 입력 */


