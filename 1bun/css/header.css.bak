@charset "utf-8";
/* CSS Document */



/*상단 유틸 시작*/

#Header .Util-Menu{width:100%; height:45px; margin:0 auto; background:url(../images/header/back_util_line.gif) repeat-x; }
#Header .Util-Menu .Util-Menu-Con{width:1000px; height:45px; margin:0 auto; }
.Util-Menu a{color:#666;}
.Util-Menu a:hover{color:#3a419b;}




.Util-Menu-Con .Left-Menu{float:left; width:415px; height:20px; margin-top:15px; margin-left:30px;}
.Util-Menu-Con .Left-Menu li{ float:left; background:url(../images/header/bullet_allow.gif) 1px 3px no-repeat; text-indent:10px; margin-left:24px;}
.Util-Menu-Con .Customer-Num{width:260px; height:37px; float:right;}



/*GNB Navi 시작*/

#Header .Navi{width:100%; height:68px;  margin:0 auto; background:url(../images/header/back_gnb_line.gif) repeat-x;}
#Header .Navi-Con{width:1000px; height:68px;  margin:0 auto;}
#Header .Navi-Con h1{float:left; width:164px; height:55px; margin-top:5px; margin-left:30px;  }

#Header .Navi-Con #Gnb{float:right; width:760px; height:51px;}

#Header .Navi-Con #Gnb ul{float:right;}
#Header .Navi-Con #Gnb li{float:left; width:120px; height:64px; }
#Header .Navi-Con #Gnb li a{display:block; width:120px; height:64px; font-size:0; text-indent:-99999px; background:url(../images/header/gnb_a.gif) no-repeat;}
#Header .Navi-Con #Gnb li a:hover{background:url(../images/header/gnb_a_over.gif) no-repeat;}

#Header .Navi-Con #Gnb li.Gnb-b a{display:block; width:120px; height:64px; background:url(../images/header/gnb_b.gif) no-repeat;}
#Header .Navi-Con #Gnb li.Gnb-b a:hover{background:url(../images/header/gnb_b_over.gif) no-repeat;}


#Header .Navi-Con #Gnb li.Gnb-c a{display:block; width:120px; height:64px; background:url(../images/header/gnb_c.gif) no-repeat;}
#Header .Navi-Con #Gnb li.Gnb-c a:hover{background:url(../images/header/gnb_c_over.gif) no-repeat;}


#Header .Navi-Con #Gnb li.Gnb-d a{display:block; width:120px; height:64px; background:url(../images/header/gnb_d.gif) no-repeat;}
#Header .Navi-Con #Gnb li.Gnb-d a:hover{background:url(../images/header/gnb_d_over.gif) no-repeat;}

#Header .Navi-Con #Gnb li.Gnb-e a{display:block; width:120px; height:64px; background:url(../images/header/gnb_e.gif) no-repeat;}
#Header .Navi-Con #Gnb li.Gnb-e a:hover{background:url(../images/header/gnb_e_over.gif) no-repeat;}

#Header .Navi-Con #Gnb li.Gnb-f a{display:block; width:120px; height:64px; background:url(../images/header/gnb_f.gif) no-repeat;}
#Header .Navi-Con #Gnb li.Gnb-f a:hover{background:url(../images/header/gnb_f_over.gif) no-repeat;}