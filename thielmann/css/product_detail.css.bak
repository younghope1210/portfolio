


@charset "utf-8";

#Product-Detail{width:100%; height:100%; margin:0 auto;  margin:0 auto; overflow:hidden; }
#Product-Detail #Hilight-Info-Con{width:100%; height:auto; margin:0 auto; position:relative;    }




/*본문시작*/


#Hilight-Info-Con .Thumbnail-Con{width:100%; height:auto;}
.Thumbnail-Con2{width:1200px; margin:30px auto; position:relative;}
.Thumbnail-Con .Thumbnail-Contents{width:500px; height:auto; margin:0 auto; overflow:hidden;}
.Thumbnail-Con .Thumbnail-Contents img{width:500px; height:auto;  overflow:hidden; }



/*슬라이더 버튼 제어*/

.slidesjs-navigation {display:block; width:65px; height:65px; line-height:65px; text-align:center; font-size:40px; background:#e1dfe0;  color:#fff; z-index:10; opacity:0.8;}
.slidesjs-navigation:hover{background:#000; color:#fff; }
	
.slidesjs-next {position:absolute; top:200px; right:150px;}
.slidesjs-previous {position:absolute; top:200px; left:150px;}

.slidesjs-pagination {display:none;}



/*상품 정보*/

.Product-Info{clear:both; width:100%; height:auto;  border-top:1px solid #cccccc; border-bottom:1px solid #cccccc; }

.Product-Info .Product-Info-Con{width:1200px; height:180px; background:#f7f6f7; margin:0 auto; border-right:1px solid #cccccc; }
.Product-Info .Product-Info-Con div{width:49.5%; height:180px; float:left; border-left:1px solid #cccccc; box-sizing:border-box; }

.Product-Info .Product-Info-Con ul{width:90%; margin:0 auto;}
.Product-Info .Product-Info-Con .Product-Info-Con-A ul{font-size:18px; margin-top:40px; color:#000;}
.Product-Info .Product-Info-Con .Product-Info-Con-A ul li{margin-bottom:20px; background:url(../images/product/bullet_red.gif) 0 6px no-repeat; text-indent:15px;}

.Product-Info .Product-Info-Con .Product-Info-Con-A ul li span{color:#cc3333;}




.Product-Info .Product-Info-Con .Product-Info-Con-B ul{font-size:18px; margin-top:20px; color:#000;  }
.Product-Info .Product-Info-Con .Product-Info-Con-B ul li{ text-align:center;margin-bottom:10px; }
.Product-Info .Product-Info-Con .Product-Info-Con-B ul li.customer{font-size:20px; height:20px; padding-top:5px; }
.Product-Info .Product-Info-Con .Product-Info-Con-B ul li.customer img{vertical-align:middle; text-align:left;}
.Product-Info .Product-Info-Con .Product-Info-Con-B ul li.Btn-Red{width:180px; height:40px; line-height:40px; background:#cc3333; margin:0 auto; color:#fff; border-radius:5px;}
.Product-Info .Product-Info-Con .Product-Info-Con-B ul li.Btn-Red a{color:#fff;}


/*상품 디테일 섹션 */



.Detail-Explain{width:100%; height:auto; padding-top:50px;}
.Detail-Explain .Detail-Explain-Con{width:1200px; margin:0 auto; }
.Detail-Explain .Detail-Explain-Con #tab{width:620px; height:74px;}
.Detail-Explain .Detail-Explain-Con #tab li{width:308px; height:74px; float:left;}
.Detail-Explain .Detail-Explain-Con #tab li a{display:block; width:308px; height:74px; background:#f7f6f7; color:#333333;  line-height:74px; text-align:center; font-size:14px; border:1px solid #cccccc;}
.Detail-Explain .Detail-Explain-Con #tab li a:hover{border:0;  color:#333333; border:1px solid #cccccc;}
.Detail-Explain .Detail-Explain-Con #tab li.active a{background:#df323a; border:0; color:#fff;  border:1px solid #df323a;} /*탭 모양을 잡아준다*/
.Detail-Explain .Detail-Explain-Con #tab li.active a:hover{background:#df323a; border:0; color:#fff; border:1px solid #df323a;}







/*상품 상세페이지*/
#Detail-Explain-Con-A{width:100%; height:auto; border:1px solid #cccccc; padding:50px 0;}

#Detail-Explain-Con-A ul.Detail_Page{width:800px; height:auto; margin:0 auto;}
#Detail-Explain-Con-A ul li{width:100%; height:auto;}
#Detail-Explain-Con-A ul li img{display:block; vertical-align:bottom;}





/*제품 구입 시 주의 사항*/

#Detail-Explain-Con-B{width:100%; height:auto; border:1px solid #cccccc; padding:50px 0;}
#Detail-Explain-Con-B p,
#Detail-Explain-Con-B ul{width:95%; margin:20px auto;}

#Detail-Explain-Con-B p.Con-Title{font-size:14px; margin-top:30px;  margin-bottom:30px;  font-weight:bold; color:#000; background:url(../images/product/bullet_red.gif) 0 6px no-repeat; text-indent:15px;  }                               }
#Detail-Explain-Con-B ul li{margin-bottom:20px; line-height:20px;}

#Detail-Explain-Con-B p span,
#Detail-Explain-Con-B p.Text-Red{color:#cc3333; line-height:20px;}

#Detail-Explain-Con-B p em{display:block; line-height:20px;}



