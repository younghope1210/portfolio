


@charset "utf-8";

#About{width:1200px; height:100%; margin:0 auto;  margin:0 auto; overflow:hidden;  }
#About #About-Con{width:1151px; height:auto; margin:0 auto; position:relative;    }
/*#Electric_Range_Explain #Electric_Range_Con img{width:100%; height:auto;}*/


/*본문시작*/



#About #About-Con h3{width:100%; height:100px; line-height:100px; border-bottom:1px solid #000; font-size:40px; font-weight:bold; color:#000;}
#About #About-Con h3 em{color:#df323a;}


.About-Explains{width:100%; height:auto; margin:0 auto; color:#000; margin:30px 0; }

.About-Explains .About-Explains-Con{width:48%; height:300px; float:left;   margin-right:15px; margin-bottom:20px;}
.About-Explains .images{width:48%; height:300px;  float:left; overflow:hidden; margin-right:15px; margin-bottom:20px;}


.About-Explains h5{font-size:36px; margin:30px 0;}

.About-Explains p.Explains{width:95%; margin-bottom:20px; font-size:20px; color:#666666; line-height:25px;}

p.Col-Red{color:#df323a;}


