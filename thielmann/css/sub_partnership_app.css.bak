


@charset "utf-8";

#Partnership-App{width:1200px; height:100%; margin:0 auto;  margin:0 auto; overflow:hidden;  }
#Partnership-App #Partnership-Info-Con{width:1151px; height:auto; margin:0 auto;  position:relative;    }



/*본문시작*/



#Partnership-App-Con h3{width:100%; height:100px; line-height:100px; border-bottom:1px solid #000; font-size:40px; font-weight:bold; color:#000;}
#Partnership-App-Con h3 em{color:#df323a;}



/* 개인정보동의 */


.Agreement{width:100%; height:auto; margin:0 auto; font-size:14px;}

.Agreement .Agreement-Title{font-size:35px; height:auto; text-align:center; padding:30px 0; color:#000;}
.Agreement .Agreement-Con{width:95%; height:100%; border:1px solid #c2c2c2; margin:0 auto; padding:30px}


.Agreement .Agreement-Con ul.Agreement-List li{margin-bottom:5px;}
.Agreement .Agreement-Con ul.Agreement-List span{color:#df323a; font-weight:bold;}

.Agreement .Agreement-Con p.Explain{margin-top:30px; margin-bottom:10px; color:#df323a; }
.Agreement .Agreement-Con .Explain-List li{margin-bottom:5px;}

.Btn-Submit{clear:both; width:100%; height:auto; margin:0 auto; }
.Btn-Submit img{display:block; margin:0 auto;  padding:50px 0; }

.Agreement-Check{margin:10px 0}
.Agreement-Check input{width:20px; height:20px; margin-left:10px; vertical-align:middle;}


/*파트너 신청하기 서식*/
.Partnership-App-tabel{width:100%; height:auto; margin:50px auto;}

.Partnership-App-tabel table{width:100%; height:auto; border-top:2px solid #df323a;}
.Partnership-App-tabel table th{width:20%; height:40px; border:1px solid #cccccc; background:#fafafa;}
.Partnership-App-tabel table td{height:40px; border:1px solid #cccccc;}
.Partnership-App-tabel table td input{width:95%; height:30px; border:1px solid #cccccc; }
.Partnership-App-tabel table td textarea{width:95%; height:500px; margin:10px 0;  border:1px solid #cccccc;}


@media (max-width: 1280px) {


#Partnership-App{width:100%; height:100%; margin:0 auto;  margin:0 auto; overflow:hidden;  }
#Partnership-App #Partnership-Info-Con{width:1008px; height:auto; margin:0 auto;  position:relative;    }


}



