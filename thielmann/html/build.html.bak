									if ( i == num) {			
										f.eq(i).addClass('active');	
										$('.menu_tab0' + i ).show();
									} else {
										f.eq(i).removeClass('active');					
										$('.menu_tab0' + i ).hide();
									}
								}
							}



							</script>



